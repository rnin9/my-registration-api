cat >> .gitignore << 'EOF'

# RubyMine / IntelliJ IDEA
.idea/
*.iml
*.iws
.idea_modules/

# VS Code
.vscode/

# Logs
/log/*
!/log/.keep
*.log

# Environment variables
.env
.env.*
!.env.example

# Rails temp
/tmp/*
!/tmp/.keep
/tmp/pids/*
!/tmp/pids/.keep

# Storage
/storage/*
!/storage/.keep

# macOS
.DS_Store

# Docker
docker-compose.override.yml
EOF